{
    "version": 2,
    "framework": null,
    "functions": {
        "api/miniproxy.js": {
            "runtime": "nodejs18.x",
            "memory": 3008,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/api/miniproxy(.*)",
            "dest": "/api/miniproxy.js?url=$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/miniproxy.js?url=$1"
        }
    ]
}
